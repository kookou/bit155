package BitStore.domain;

public class DiscardProductVO {
	private int discardProductNo;
	private String discardProductName;
	private String discardExprtDate;
	private int discardPrice;

	public DiscardProductVO(int discardProductNo, String discardProductName, String discardExprtDate,
			int discardPrice) {
		super();
		this.discardProductNo = discardProductNo;
		this.discardProductName = discardProductName;
		this.discardExprtDate = discardExprtDate;
		this.discardPrice = discardPrice;
	}

	public void setDiscardProductNo(int discardProductNo) {
		this.discardProductNo = discardProductNo;
	}

	public String getDiscardProductName() {
		return discardProductName;
	}

	public void setDiscardProductName(String discardProductName) {
		this.discardProductName = discardProductName;
	}

	public String getDiscardExprtDate() {
		return discardExprtDate;
	}

	public void setDiscardExprtDate(String discardExprtDate) {
		this.discardExprtDate = discardExprtDate;
	}

	public int getDiscardPrice() {
		return discardPrice;
	}

	public void setDiscardPrice(int discardPrice) {
		this.discardPrice = discardPrice;
	}

	@Override
	public String toString() {
		return "DiscardProduct [discardProductNo=" + discardProductNo + ", discardProductName=" + discardProductName
				+ ", discardExprtDate=" + discardExprtDate + ", discardPrice=" + discardPrice + "]";
	}

}